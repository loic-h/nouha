extends ./layout.jade

block content
	h1 index
	div#images
		|#{images}
	div.upload
		form(
			action="/upload"
			method="post"
			enctype="multipart/form-data"
		)
			input(
				type="file"
				name="images"
				multiple="multiple"
			)
			input(
				type="submit"
				value="go"
			)
			div.gallery
